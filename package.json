	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/node": "^7.12.1",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@types/node": "^12.19.1",
		"babel-eslint": "^10.1.0",
